{
    "name": "home/kitee-bundle",
    "type": "contao-bundle",
    "description": "Adds Kitee to Contao 4",
    "keywords":["contao","kitee"],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "HolsteinMedia",
            "homepage": "http://www.holsteinmedia.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.1|^8.1",
        "contao/core-bundle": "^4.4.2",
		"jrgregory/m17-sticky-backend-footer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Home\\KiteeBundle\\": ""
        },
        "classmap": [
            "Resources/contao/"
        ],
        "exclude-from-classmap": [
            "Resources/contao/config/",
            "Resources/contao/templates/"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Home\\KiteeBundle\\ContaoManager\\Plugin"
    }
}
